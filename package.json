{
  "name": "websklep",
  "version": "1.0.0",
  "description": "Shopping Cart App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:feed": "node backend/seeder",
    "data:remove": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daniel-Badura/WebSklep.git"
  },
  "keywords": [
    "MERN",
    "E-Commerce",
    "Backend"
  ],
  "author": "Daniel Badura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Daniel-Badura/WebSklep/issues"
  },
  "homepage": "https://github.com/Daniel-Badura/WebSklep#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "handlebars": "^4.7.7",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.2",
    "nodemailer": "^6.9.1",
    "react-router-dom": "^6.10.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21"
  }
}
